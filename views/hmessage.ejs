<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Profile</title>

    <!-- Bootstrap core CSS -->
    <link href="assets/dashboard/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="assets/dashboard/css/simple-sidebar.css" rel="stylesheet">
<!-- profile -->
    <link href="assets/dashboard/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <script src="assets/dashboard/css/bootstrap.min.js"></script>

    <script src="assets/dashboard/css/jquery.min.js"></script>

</head>

<body>

    <div class="d-flex" id="wrapper">

        <!-- Sidebar -->
        <div class="bg-light border-right" id="sidebar-wrapper">
            <div class="sidebar-heading">NPM Hotel</div>
            <div class="list-group list-group-flush">
                <a href="/dashboard" class="list-group-item list-group-item-action bg-light">Dashboard</a>
                <a href="/room" class="list-group-item list-group-item-action bg-light">Room</a>
                <a href="/banquet" class="list-group-item list-group-item-action bg-light">Banquet</a>
                <a href="#" class="list-group-item list-group-item-action bg-light">Events</a>
                <a href="/dashboardprofile" class="list-group-item list-group-item-action bg-light">Profile</a>
                <a href="/hmessage" class="list-group-item list-group-item-action bg-light">Message</a>
            </div>
        </div>
        <!-- /#sidebar-wrapper -->

        <!-- Page Content -->
        <div id="page-content-wrapper">

      <% for (var msg of message){ %>
          <div class="card" style="width: 18rem;">
            <div class="card-body">
              <h5 class="card-title"><%= msg.mname%></h5>
              <h6 class="card-subtitle mb-2 text-muted"><%= msg.memail%></h6>
              <p class="card-text"><%= msg.mmessage%></p>
              <!-- <a href="#" class="card-link">Card link</a>
              <a href="#" class="card-link">Another link</a> -->
            </div>
          </div>
        <% } %>
        </div>
        <!-- /#page-content-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- Bootstrap core JavaScript -->
    <script src="assets/dashboard/vendor/jquery/jquery.min.js"></script>
    <script src="assets/dashboard/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

</body>

</html>
